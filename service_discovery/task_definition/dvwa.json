[
  {
      "name": "dvwa",
      "essential": true,
      "image": "vulnerables/web-dvwa",
      "entryPoint": ["/main.sh"],
      "command": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-region": "${region}",
          "awslogs-group": "${log_group_name}",
          "awslogs-stream-prefix": "${log_stream_prefix}"
        }
      },
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ]
  }
]